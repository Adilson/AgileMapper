{
  "version": "0.6.1-*",
  "authors": [ "Steve Wilkes" ],
  "title": "AgileMapper",
  "name": "AgileObjects.AgileMapper",
  "description": "A zero-configuration, highly-configurable, portable object-object mapper with viewable execution plans. Performs deep clones, updates and merges via a static or instance API.",
  "copyright": "2016 Steve Wilkes",
  "packOptions": {
    "copyright": "2016 Steve Wilkes",
    "owners": [ "Steve Wilkes" ],
    "licenseUrl": "https://github.com/agileobjects/AgileMapper/blob/master/LICENCE.md",
    "projectUrl": "https://github.com/AgileObjects/AgileMapper",
    "requireLicenseAcceptance": false,
    "summary": "A zero-configuration object mapper with viewable execution plans",
    "tags": [ "Mapping, Mapper, ViewModel" ],
    "repository": {
      "type": "git",
      "url": "https://github.com/AgileObjects/AgileMapper"
    }
  },

  "buildOptions": {
    "keyFile": "..\\AgileMapper.snk",
    "warningsAsErrors": true,
    "xmlDoc": true,
    "outputName": "AgileObjects.AgileMapper"
  },

  "dependencies": {
    "AgileObjects.NetStandardPolyfills": "0.1.2",
    "AgileObjects.ReadableExpressions": "1.8.0"
  },

  "frameworks": {
    "netstandard1.0": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Dynamic.Runtime": "4.0.11"
      },
      "buildOptions": {
        "define": [ "NET_STANDARD" ]
      }
    },
    "net40": {}
  }
}
